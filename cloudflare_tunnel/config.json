{
  "name": "Cloudflare Tunnel",
  "version": "1.6.0",
  "slug": "cloudflare_tunnel",
  "description": "Cloudflare Tunnel for Home Assistant",
  "url": "https://github.com/w35l3y/hassio-addons/tree/main/cloudflare_tunnel",
  "ingress": false,
  "init": false,
  "apparmor": true,
  "panel_icon": "mdi:tunnel",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "map": ["config:rw", "ssl:rw"],
  "startup": "services",
  "boot": "auto",
  "ports": {
    "41705/tcp": null
  },
  "options": {
    "tunnel": "homeassistant",
    "credentials-file": "/data/tunnel.json",
    "origincert": "/data/cert.pem",
    "metrics": "localhost:41705",
    "no-autoupdate": true,
    "originRequest": {},
    "ingress": [
      {
        "hostname": "",
        "service": "http://homeassistant:8123"
      },
      {
        "service": "http_status:404"
      }
    ]
  },
  "schema": {
    "autoupdate-freq": "str?",
    "no-autoupdate": "bool?",
    "origincert": "str?",
    "grace-period": "str?",
    "metrics": "str?",
    "metrics-update-freq": "str?",
    "tag": "str?",
    "retries": "int(0,)?",
    "pidfile": "str?",
    "protocol": "str?",
    "region": "str?",
    "logfile": "str?",
    "loglevel": "str?",
    "transport-loglevel": "str?",
    "tunnel": "str?",
    "credentials-file": "str?",
    "originRequest": {
      "connectTimeout": "str?",
      "tlsTimeout": "str?",
      "tcpKeepAlive": "str?",
      "noHappyEyeballs": "bool?",
      "keepAliveConnections": "int(0,)?",
      "keepAliveTimeout": "str?",
      "httpHostHeader": "str?",
      "originServerName": "str?",
      "caPool": "str?",
      "noTLSVerify": "bool?",
      "disableChunkedEncoding": "bool?",
      "proxyAddress": "str?",
      "proxyPort": "port?",
      "proxyType": "str?"
    },
    "url": "str?",
    "ingress": [
      {
        "hostname": "str?",
        "path": "str?",
        "service": "str?"
      }
    ]
  }
}
